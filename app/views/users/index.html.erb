<div class="container mt-5">
  <h1 class="text-center mb-4">Users</h1>
  <div class="list-group">
    <% @users.each do |user| %>
      <a href="<%= user_path(user) %>" class="list-group-item list-group-item-action">
        <%= user.name %>
      </a>
    <% end %>
  </div>
</div>
